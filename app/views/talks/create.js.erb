$("#talk-area").html("<%= escape_javascript(render('shared/talk_list', talks: @talk_list)) %>");
$("#talk-area").removeClass("hidden");
$(".tabmenu").removeClass("active");
$("#talk-tab").addClass("active");
$("#talk-form").removeClass("hidden");

<% flash.each do |message_type, message| %>
  $.smkAlert({text:'<%= message %>', type:'<%= message_type %>'});
  <% flash.discard message_type %>
<% end %>
